{
  "name": "nevernii-otvet",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "start": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore && stylelint \"**/*.scss\" --fix",
    "format": "prettier -w \"src/**/*.{js,ts,vue,scss}\""
  },
  "dependencies": {
    "axios": "^1.3.4",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@fruktorum/prettier-config": "^1.0.0",
    "@fruktorum/prettier-config-pug": "^1.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "pug": "^3.0.2",
    "sass": "^1.58.3",
    "stylelint": "^15.2.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-stylelint": "^4.2.0",
    "vitest": "^0.29.1",
    "vue-pug-plugin": "^2.0.3",
    "vue-tsc": "^1.2.0"
  }
}
